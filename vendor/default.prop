#
# Properties to overwrite (Default)
#

# disable "keep SIM alive" in LPM feature
#persist.radio.apm_sim_not_pwdn=0

# enable restoring mode_pref when manual select network with rat
#persist.radio.restore_mode_pref=1

# hide internal OOS for 5s when modem is camping
#persist.radio.ignore_dom_time=5

# set default multisim config to dsds
persist.radio.multisim.config=dsds

#
# PROPERTIES FOR IMS
#
persist.dbg.ims_volte_enable=1
persist.dbg.volte_avail_ovr=1
persist.dbg.vt_avail_ovr=1
persist.radio.videopause.mode=1
persist.data.iwlan.enable=true
persist.dbg.wfc_avail_ovr=1

# enable face beautification
#persist.ts.postmakeup=true
#persist.ts.rtmakeup=true

# disable CM's blacklist
#ro.telephony.cm_blklst_on=0

persist.regional.wipedata.level=all

# enable mbn file update for SIMTrigger
persist.radio.sw_mbn_update=0
persist.radio.hw_mbn_update=0

# enable SIMTrigger first-only feature
#persist.radio.trigger.firstonly=true

# enable SIMTrigger silence mode feature
#persist.radio.trigger.silence=true

#Default for Settings.Global.MOBILE_DATA
ro.com.android.mobiledata=true
# Default for Settings.Global.DATA_ROAMING
ro.com.android.dataroaming=false
# enable the feature for RJIL,vowifi quality, embms
persist.radio.atel.carrier=405854
# Dennis 2016/5/19 add for Bug 10100 [Dialer]3.2 Phone number display issue after call forward
persist.radio.redir_party_num=0

#Additional properties for RJIL
persist.radio.detect4gcard=true
persist.radio.primarycard=true
ro.telephony.default_network=9

# Dennis 2016/5/23 add for Bug 10115 [Dialer]3.16 Wi-Fi calling and Video Telephony support
persist.radio.NO_STAPA=1

#Modem should not shut down in Airplane mode
persist.radio.apm_mdm_not_pwdn=1

# yujing.lin 2016/6/3 add for Bug 10684 - The value of persist.env.contacts.autosync is not ture
persist.env.contacts.autosync=true

#Support manual scan
persist.radio.plmn_disable_data=false
#qing.liu 2016/8/8 add to support omacp
persist.mmssupportcp.enable=true
persist.omacp.enable=true
